    "patientName": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "startTime": {
      "type": "string",
      "required": true
    },
    "endTime": {
      "type": "string",
      "required": true
    },
    "doctor": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "comments": {
      "type": "string",
